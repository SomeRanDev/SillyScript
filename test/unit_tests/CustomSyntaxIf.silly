def CameraMove(start: int, end: int) -> dict!action:
	type: 0;
	start: start;
	end: end;

def StartAnimation(nodePath: string, animationName: string) -> dict!action:
	type: 1;
	nodePath: nodePath;
	animationName: animationName;

syntax If:
	pattern:
		if <condition: dict!bool>:
			<things: dict!action list>

syntax Compare:
	pattern -> dict!bool:
		<first: int> == <second: int>


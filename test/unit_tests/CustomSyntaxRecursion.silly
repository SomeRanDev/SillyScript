syntax PrefixQuestionNumber:
	pattern:
		?<number: int>

	pattern:
		?<input: syntax!PrefixQuestionNumber>

?123;
???????321;

syntax SaySomething:
	pattern:
		say <something: string>

	pattern:
		<exclaimed: syntax!SaySomething>!

	pattern:
		<questioned: syntax!SaySomething>?

say "What";
say "What"?;
say "OMG"??!?!?!;

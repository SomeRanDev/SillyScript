syntax If:
    pattern:
        if <condition: dict!condition>:
            <contents: dict!action list>

	### Transmutes ###
    def transmute(condition: dict, contents: list) -> dict!action:
        type: 2;
        condition: condition;
        if_true: contents;
        if_false: null;